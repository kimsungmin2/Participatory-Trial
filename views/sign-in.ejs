<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/css/sign-in.css" />
    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    <link
      rel="stylesheet"
      href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css"
    />
    <title>sign in</title>
  </head>
  <body>
    <%- include('partials/nav') %>
    <main>
      <form id="signinForm">
        <div class="email">
          <label for="email">email</label>
          <input
            id="email"
            class="emailInput"
            type="email"
            name="email"
            placeholder="example@google.com"
            autofocus
          />
        </div>
        <div class="password">
          <label for="password">password</label>
          <input
            id="password"
            class="passwordInput"
            type="text"
            name="password"
            placeholder="password"
          />
        </div>
        <button id="submitBtn" class="submitBtn" type="submit">로그인</button>
      </form>
      <div id="shift">
        <button class="Btn" type="button" onclick="location.href='/sign-up'">
          회원가입
        </button>
        <button class="Btn" type="button" onclick="location.href='/naver'">
          네이버 로그인
        </button>
        <button class="Btn" type="button" onclick="location.href='/kakao'">
          카카오 로그인
        </button>
        <button class="Btn" type="button" onclick="location.href='/google'">
          구글 로그인
        </button>
      </div>
    </main>
    <footer>footer</footer>
    <script src="/scripts/sign-in.js"></script>
  </body>
</html>
